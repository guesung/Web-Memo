{
	"extends": "@web-memo/tsconfig/react",
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"@*": ["./*"]
		},
		"moduleResolution": "bundler",
		"noEmit": false,
		"emitDeclarationOnly": false,
		"strictPropertyInitialization": false,
		"stripInternal": true,
		"declarationDir": "dist",
		"outDir": "dist"
	},
	"include": ["src"],
	"exclude": ["dist"]
}
