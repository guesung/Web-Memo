{
  "extends": "@extension/tsconfig/react",
  "compilerOptions": {
    "rootDir": ".",
    "baseUrl": ".",
    "paths": {
      "@*": ["./*"]
    },
    "noEmit": false,
    "emitDeclarationOnly": false,
    "strictPropertyInitialization": false,
    "stripInternal": true,
    "declarationDir": "dist",
    "outDir": "dist"
  },
  "include": ["src"],
  "exclude": ["dist"]
}
