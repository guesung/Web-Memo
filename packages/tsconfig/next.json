{
	"$schema": "https://json.schemastore.org/tsconfig",
	"display": "Next.js",
	"extends": "./react.json",
	"compilerOptions": {
		"plugins": [{ "name": "next" }],
		"allowJs": true,
		"incremental": true,
		"noEmit": true,
		"module": "ESNext",
		"moduleResolution": "bundler",
		"jsx": "preserve"
	}
}
