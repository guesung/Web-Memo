{
  "extends": "@extension/tsconfig/base",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@src/*": ["./src/*"]
    },
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "target": "ES2015"
  },
  "include": ["src", ".next/types/**/*.ts"],
  "exclude": ["node_modules", ".next"]
}
