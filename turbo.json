{
	"$schema": "https://turbo.build/schema.json",
	"ui": "tui",
	"tasks": {
		"ready": {
			"dependsOn": ["^ready"],
			"outputs": ["dist", "build"],
			"cache": false
		},
		"dev": {
			"dependsOn": ["^ready"],
			"outputs": ["dist", "build"],
			"inputs": [".env"],
			"persistent": true,
			"cache": false
		},
		"preview": {
			"cache": false
		},
		"build": {
			"dependsOn": ["ready", "^build"],
			"outputs": ["dist", "build"],
			"inputs": [".env"]
		},
		"start": {
			"outputs": ["dist", "build", ".next"],
			"inputs": [".env"]
		},
		"zip": {
			"dependsOn": ["^build"],
			"cache": false
		},
		"type-check": {
			"dependsOn": ["^ready"],
			"cache": false
		},
		"lint": {
			"cache": false
		},
		"lint:fix": {
			"cache": false
		},
		"clean": {
			"cache": false
		},
		"tsc-watch": {
			"cache": false
		}
	}
}
