{
	"$schema": "https://turbo.build/schema.json",
	"ui": "tui",
	"globalPassThroughEnv": ["COREPACK_HOME"],
	"tasks": {
		"ready": {
			"dependsOn": ["^ready"],
			"outputs": ["dist/**", "build/**"]
		},
		"dev": {
			"dependsOn": ["^ready"],
			"persistent": true,
			"cache": false
		},
		"preview": {
			"persistent": true,
			"cache": false
		},
		"build": {
			"dependsOn": ["ready", "^build"],
			"outputs": ["dist/**", "build/**", ".next/**", "!.next/cache/**"],
			"inputs": ["$TURBO_DEFAULT$", ".env"]
		},
		"start": {
			"dependsOn": ["build"],
			"persistent": true,
			"cache": false
		},
		"zip": {
			"dependsOn": ["^build"],
			"cache": false
		},
		"type-check": {
			"dependsOn": ["^ready"],
			"outputs": []
		},
		"lint": {
			"outputs": []
		},
		"lint:fix": {
			"cache": false
		},
		"clean": {
			"cache": false
		},
		"tsc-watch": {
			"persistent": true,
			"cache": false
		}
	}
}
